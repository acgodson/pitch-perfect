{
  "name": "pitch-perfect-client",
  "version": "1.0.9",
  "type": "module",
  "private": true,
  "main": "dist/agent.js",
  "module": "dist/agent.js",
  "types": "dist/agent.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/agent.d.ts",
        "default": "./dist/agent.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@chainlink/ccip-js": "file:../ccip-javascript-sdk/packages/ccip-js",
    "@chainlink/contracts-ccip": "^1.2.1",
